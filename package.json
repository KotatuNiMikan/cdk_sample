{
  "name": "spa_sample",
  "version": "0.1.0",
  "bin": {
    "cdk_sample": "bin/cdk_sample.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test-lambda": "poetry -C lambda run pytest",
    "cdk": "cdk",
    "prepare": "husky && npm run prepare-lambda",
    "prepare-lambda": "poetry -C lambda install --all-group"
  },
  "lint-staged": {
    "*.{ts,json,md}": [
      "dprint fmt",
      "dprint check"
    ],
    "*.py": ["poetry -C lambda run ruff format", "poetry -C lambda run ruff check"]
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "22.15.18",
    "aws-cdk": "2.1016.0",
    "dprint": "^0.49.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.0.0",
    "ts-jest": "^29.3.3",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3"
  },
  "dependencies": {
    "@aws-cdk/aws-lambda-python-alpha": "^2.196.0-alpha.0",
    "aws-cdk-lib": "2.196.0",
    "change-case-all": "^2.1.0",
    "constructs": "^10.4.2",
    "source-map-support": "^0.5.21"
  }
}
